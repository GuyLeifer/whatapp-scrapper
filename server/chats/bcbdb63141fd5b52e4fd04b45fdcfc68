19.10.2020, 15:41 - שחר אליהו: from time import sleep
from selenium import webdriver
from webdriver_manager.chrome import ChromeDriverManager
chrome_options = webdriver.ChromeOptions()
prefs = {"profile.default_content_setting_values.notifications" : 2}
chrome_options.add_experimental_option("prefs",prefs)
driver = webdriver.Chrome(ChromeDriverManager().install(),options=chrome_options)
password=""
username=""

driver.get("https://microphone-kodak-inspections-slow.trycloudflare.com/login")
sleep(2)

driver.find_element_by_xpath("/html/body/div[1]/div/div/form/dl/dt[1]/input").send_keys(username)
driver.find_element_by_xpath("/html/body/div[1]/div/div/form/dl/dt[2]/input").send_keys(password)
driver.find_element_by_xpath("/html/body/div[1]/div/div/form/dl/dt[2]/button").click()
sleep(2)
driver.find_element_by_xpath("/html/body/ul[1]/li[23]/div/a").click()
sleep(2)
driver.find_element_by_xpath("/html/body/div[1]/div/div/a").click()
sleep(2)
previusLinks= []
def getAllLinks():
    if driver.current_url in previusLinks:
        driver.close()
        return
    previusLinks.append(driver.current_url)
    elems = driver.find_elements_by_xpath("//a[@href]")
    if len(elems)>0:
        for elem in elems:
            link = elem.get_attribute("href")
            driver.execute_script(str("window.open(" + "'" + link + "'" + ",'_blank');"))
            sleep(1)
            try:
                # driver.find_element_by_tag_name("img")
                allImgs= driver.execute_script(("document.querySelectorAll(img)"))
                if len(allImgs)>0 :
                    print(driver.current_url)
            except:
                pass
    else:
        print(driver.current_url)
        driver.close()


elems = driver.find_elements_by_xpath("//a[@href]")
while len(driver.window_handles)>0:
    for x in range(len(driver.window_handles)):
        driver.switch_to.window(driver.window_handles[x])
        getAllLinks()
